    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@redux-devtools/core": "^3.13.1",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.2.0",
