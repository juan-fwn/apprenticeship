{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.3",
    "@testing-library/user-event": "^13.5.0",
    "dotenv": "^16.0.1",
